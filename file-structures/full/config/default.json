{
  "authentication": {
    "on": true
  },
  "environment_variables" : {
    "http_port": {
      "key": "APP_PORT",
      "default": "3010"
    },
    "session_secret": {
      "key": "APP_SESSION_SECRET",
      "default":"somethingverysecret"
    }
  },
  "site" : {
    "site_name": "Base node application"
  },
  "template_tags" : {
    "blockStart": "{%",
    "blockEnd": "%}",
    "variableStart": "{{",
    "variableEnd": "}}",
    "commentStart": "{#",
    "commentEnd": "#}"
  }
}